<?xml version="1.0"?>

<launch>

  <!-- General Parameters -->

  <arg name="dispatch_plan" default="true" />
  <arg name="save_result" default="true" />




  <node name="task_planner_node" pkg="task_planner" type="main.py"  output="screen" >
    <rosparam command="load" file="$(find hrc_mosaic_task_planning_interface)/config/task_goal_example.yaml" />
    <rosparam command="load" file="$(find hrc_mosaic_task_planning_interface)/config/task_planning_optimization.yaml" />

    <param name="dispatch_plan" value="$(arg dispatch_plan)" />
    <param name="save_result" value="$(arg save_result)" />
    <param name="result_file_path" value="$(find hrc_mosaic_task_planning_interface)/solutions/recipe_solution.yaml" />

    <!-- <rosparam command="load" file="$(find task_planner)/config/task_planning_optimization.yaml" ns="optimization_parameters/"/> -->
  </node>

</launch>
